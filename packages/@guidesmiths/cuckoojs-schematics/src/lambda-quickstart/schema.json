{
  "$schema": "http://json-schema.org/schema",
  "$id": "SchematicsLambdaQuickstart",
  "title": "Lambda Quickstart Options Schema",
  "type": "object",
  "properties": {
    "directory": {
      "type": "string",
      "description": "quickstart destination directory",
      "default": ".",
      "x-prompt": "Where do you want to create the project?"
    },
    "serviceName": {
      "type": "string",
      "description": "service name",
      "x-prompt": "What is the service name?"
    }
  },
  "required": ["directory", "serviceName"]
}
